{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://janu-ecommerce-backend.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
} 